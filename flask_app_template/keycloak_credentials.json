{
    "web": {
        "client_id": "<keycloak_client_id>",
        "client_secret": "<keycloak_client_secret>",
        "auth_uri": "https://<keycloak_url>/realms/<realm_name>/protocol/openid-connect/auth",
        "token_uri": "https://<keycloak_url>/realms/<realm_name>/protocol/openid-connect/token",
        "issuer": "https://<keycloak_url>/realms/<realm_name>",
        "userinfo_uri": "https://<keycloak_url>/realms/<realm_name>/protocol/openid-connect/userinfo",
        "redirect_uris": [
            "https://<app_domain>/oidc/callback"
        ],
        "end_session_endpoint": "https://<keycloak_url>/realms/<realm_name>/protocol/openid-connect/logout"
    }
}
